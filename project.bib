	keywords = {Genome, Transcriptome, Molecular Sequence Annotation, Protein Isoforms, Sequence Analysis, DNA, Repetitive Sequences, Nucleic Acid, De novo assembly, DNA Methylation, Gene isoform detection, Hybrid sequencing, Methylation, Third-generation sequencing},
	pages = {278--289},
	file = {Full Text:/home/elenacampoy/Zotero/storage/YZGXQRPY/Rhoads and Au - 2015 - PacBio Sequencing and Its Applications.pdf:application/pdf}
