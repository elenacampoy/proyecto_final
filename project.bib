	month = jul,
	year = {2013},
	pmid = {24179701},
	pmcid = {PMC3811116},
	keywords = {assembly, CCS read, PacBio, pass number, quality control (QC), quality value (QV), SVM regression},
	file = {Full Text:/home/elenacampoy/Zotero/storage/YZ6Y27BN/Jiao et al. - 2013 - A Benchmark Study on Error Assessment and Quality .pdf:application/pdf}
